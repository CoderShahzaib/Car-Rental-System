            "minimum": 0,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "CreateCarDTO": {
        "required": [
          "brand",
          "color",
          "model",
          "registrationNo"
        ],
        "type": "object",
        "properties": {
          "brand": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "model": {
            "maxLength": 50,
            "minLength": 0,
